{% extends "::base.html.twig" %}

    {% block conditionIcon %}
        if( listeReports[i][3] <= 20 ){
          image.url = 'images/tick/1468858867_icon-68-thermometer-quarter.png'
        }
        else if (listeReports[i][3] > 20 && listeReports[i][3] <= 30 ){
          image.url = 'images/tick/1468858886_icon-69-thermometer-half.png';

        }
        else if (listeReports[i][3] > 30) {
          image.url = "images/tick/1468858877_icon-70-thermometer-three-quarters.png"
        }
{% endblock %}
