{% extends "::base.html.twig" %}

  {% block conditionIcon %}
      if(listeReports[i][4] === 'Pas de Vent' ){
        image.url = "images/tick/sunny.png";
      }
      else if(listeReports[i][4] === 'Vent modéré' ) {
        image.url = "images/tick/weather-1.png";
      }
      else if(listeReports[i][4] === 'Vent assez fort') {
        image.url = "images/tick/weather-2.png";
      }
      else if(listeReports[i][4] === 'Vent fort') {
        image.url = "images/tick/weather-3.png";
      }
      else if(listeReports[i][4] === 'Cyclone') {
        image.url = "images/tick/weather.png";
      }
      {% endblock %}
